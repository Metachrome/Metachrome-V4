{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.2.17"
    }
  },
  "rewrites": [
    {
      "source": "/api/admin/(.*)",
      "destination": "/api/admin"
    },
    {
      "source": "/api/superadmin/(.*)",
      "destination": "/api/superadmin"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
